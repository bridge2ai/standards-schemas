@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix standards_usecase_schema: <https://w3id.org/bridge2ai/standards-usecase-schema/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/bridge2ai/standards-usecase-schema> a owl:Ontology ;
    rdfs:label "standards-usecase-schema" ;
    IAO:0000700 standards_usecase_schema:NamedThing,
        standards_usecase_schema:UseCaseCollection ;
    dcterms:license "MIT" ;
    dcterms:title "standards-usecase-schema" ;
    rdfs:seeAlso "https://bridge2ai.github.io/standards-usecase-schema" ;
    linkml:generation_date "2023-01-12T17:20:19" ;
    linkml:metamodel_version "1.7.0" ;
    linkml:source_file "standards_usecase_schema.yaml" ;
    linkml:source_file_date "2023-01-12T17:11:45" ;
    linkml:source_file_size 2297 .

linkml:SubsetDefinition a owl:Class ;
    rdfs:label "subset_definition" .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

standards_usecase_schema:UseCaseCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "UseCaseCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom standards_usecase_schema:UseCase ;
            owl:onProperty standards_usecase_schema:entries ] ;
    skos:definition "A holder for UseCase objects" .

standards_usecase_schema:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" .

standards_usecase_schema:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    rdfs:range standards_usecase_schema:UseCase .

standards_usecase_schema:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range standards_usecase_schema:PersonStatus ;
    skos:definition "living or dead status" .

PATO:0001421 a owl:Class,
        standards_usecase_schema:PersonStatus ;
    rdfs:label "ALIVE" .

PATO:0001422 a owl:Class,
        standards_usecase_schema:PersonStatus ;
    rdfs:label "DEAD" .

schema:birthDate a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:exactMatch schema:birthDate .

schema:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable description for a thing" ;
    skos:exactMatch schema:description .

schema:email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    rdfs:range linkml:String ;
    skos:definition "The main email address of a person" ;
    skos:exactMatch schema:email .

schema:identifier a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:exactMatch schema:identifier .

schema:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable name for a thing" ;
    skos:exactMatch schema:name .

standards_usecase_schema:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:description ],
        [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema:Thing .

<https://w3id.org/bridge2ai/standards-usecase-schema/PersonStatus#UNKNOWN> a owl:Class,
        standards_usecase_schema:PersonStatus ;
    rdfs:label "UNKNOWN" .

standards_usecase_schema:UseCase a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "UseCase" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:email ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Date ;
            owl:onProperty schema:birthDate ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass standards_usecase_schema:PersonStatus ;
            owl:onProperty standards_usecase_schema:vital_status ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty standards_usecase_schema:age_in_years ],
        standards_usecase_schema:NamedThing ;
    skos:definition "Represents a UseCase" .

linkml:ClassDefinition a owl:Class ;
    rdfs:label "class_definition" .

standards_usecase_schema:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "PersonStatus" ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/bridge2ai/standards-usecase-schema/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/bridge2ai/standards-usecase-schema/PersonStatus#UNKNOWN> .

linkml:SlotDefinition a owl:Class ;
    rdfs:label "slot_definition" .


