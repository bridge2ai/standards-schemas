B2AI_STANDARD:201 schema1:description "International standard for medical imaging information and related data exchange. DICOM enables the integration of medical imaging devices, PACS, and other systems from multiple manufacturers into a picture archiving and communication system." ;
    schema1:name "DICOM (Digital Imaging and Communications in Medicine)" ;
    B2AI_STANDARD:collection "clinicaldata",
        "datamodel",
        "implementation_maturity_production",
        "standards_process_maturity_final" ;
    B2AI_STANDARD:concerns_data_topic B2AI_TOPIC:15 ;
    B2AI_STANDARD:formal_specification "https://github.com/dicom/dicom-standard"^^xsd:anyURI ;
    B2AI_STANDARD:has_relevant_organization B2AI_ORG:200,
        B2AI_ORG:201 ;
    B2AI_STANDARD:is_open true ;
    B2AI_STANDARD:publication "doi:10.1148/radiology.148.1.6856849"^^xsd:anyURI ;
    B2AI_STANDARD:purpose_detail "Facilitates storage, transmission, and display of medical images across heterogeneous systems while preserving clinical context and patient information." ;
    B2AI_STANDARD:requires_registration false ;
    B2AI_STANDARD:responsible_organization B2AI_ORG:200 ;
    B2AI_APP:category "B2AI_STANDARD:DataStandard" ;
    B2AI_APP:contribution_date "2024-07-10"^^xsd:date ;
    B2AI_APP:contributor_github_name "athompson" ;
    B2AI_APP:contributor_name "Alexandra Thompson" ;
    B2AI_APP:contributor_orcid "ORCID:0000-0002-5555-6666"^^xsd:anyURI ;
    B2AI_APP:has_application B2AI_APP:201,
        B2AI_APP:202,
        B2AI_APP:203 ;
    B2AI_APP:url "https://www.dicomstandard.org/"^^xsd:anyURI ;
    B2AI_APP:used_in_bridge2ai true .

B2AI_APP:201 schema1:description "Deep learning system that analyzes chest CT images in DICOM format to automatically detect and characterize pulmonary nodules, assisting radiologists in early lung cancer screening programs." ;
    schema1:name "AI-Assisted Lung Nodule Detection in CT Scans" ;
    B2AI_APP:category "B2AI:Application" ;
    B2AI_APP:contribution_date "2024-10-20"^^xsd:date ;
    B2AI_APP:contributor_name "Dr. Sarah Chen" ;
    B2AI_APP:contributor_orcid "ORCID:0000-0003-1111-2222"^^xsd:anyURI ;
    B2AI_APP:datasheet "https://example.com/lung-nodule-ai-model-card.pdf"^^xsd:anyURI ;
    B2AI_APP:references "https://doi.org/10.1000/lung.nodule.ai.2024"^^xsd:anyURI .

B2AI_APP:202 schema1:description "Convolutional neural network pipeline for automatic segmentation and classification of brain tumors in DICOM MRI sequences, supporting treatment planning and surgical navigation." ;
    schema1:name "Automated Brain Tumor Segmentation" ;
    B2AI_APP:category "B2AI:Application" ;
    B2AI_APP:contribution_date "2024-09-15"^^xsd:date ;
    B2AI_APP:contributor_name "Dr. Michael Park" ;
    B2AI_APP:contributor_orcid "ORCID:0000-0003-2222-3333"^^xsd:anyURI ;
    B2AI_APP:datasheet "https://example.com/brain-tumor-segmentation-datasheet.pdf"^^xsd:anyURI ;
    B2AI_APP:references "https://arxiv.org/abs/2403.56789"^^xsd:anyURI,
        "https://doi.org/10.1000/brain.tumor.seg.2024"^^xsd:anyURI .

B2AI_APP:203 schema1:description "Multi-task learning model for detecting diabetic retinopathy, age-related macular degeneration, and glaucoma from DICOM-formatted fundus photographs, enabling automated screening in ophthalmology clinics." ;
    schema1:name "Retinal Disease Classification from Fundus Images" ;
    B2AI_APP:category "B2AI:Application" ;
    B2AI_APP:contribution_date "2024-08-30"^^xsd:date ;
    B2AI_APP:contributor_name "Dr. James Liu" ;
    B2AI_APP:contributor_orcid "ORCID:0000-0003-3333-4444"^^xsd:anyURI ;
    B2AI_APP:references "https://doi.org/10.1000/retinal.ai.2024"^^xsd:anyURI .

