@prefix STANDARDSDATASUBSTRATE: <https://w3id.org/bridge2ai/standards-datasubstrate-schema/> .
@prefix ns1: <schema:> .
@prefix ns2: <https://w3id.org/bridge2ai/standards-schema/:> .

STANDARDSDATASUBSTRATE:53 a ns2:DataSubstrate ;
    ns2:EDAM_ID "edam.format:3003" ;
    ns2:NCIT_ID "ncit:C153367" ;
