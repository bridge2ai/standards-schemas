@prefix B2AI_APP: <https://w3id.org/bridge2ai/standards-schema/> .
@prefix B2AI_ORG: <https://w3id.org/bridge2ai/standards-organization-schema/> .
@prefix B2AI_STANDARD: <https://w3id.org/bridge2ai/standards-datastandardortool-schema/> .
@prefix B2AI_TOPIC: <https://w3id.org/bridge2ai/standards-datatopic-schema/> .
@prefix schema1: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

B2AI_STANDARD:105 schema1:description "Fast Healthcare Interoperability Resources (FHIR) is a standard for exchanging healthcare information electronically. It provides a modular framework of components called resources that can be assembled into working systems to solve clinical and administrative problems. FHIR combines the best features of HL7's v2, v3 and CDA product lines while leveraging the latest web standards." ;
    schema1:name "HL7 FHIR" ;
    B2AI_STANDARD:collection "clinicaldata",
        "datamodel" ;
    B2AI_STANDARD:concerns_data_topic B2AI_TOPIC:4,
        B2AI_TOPIC:5 ;
    B2AI_STANDARD:formal_specification "https://www.hl7.org/fhir/overview.html"^^xsd:anyURI ;
    B2AI_STANDARD:has_relevant_organization B2AI_ORG:50 ;
    B2AI_STANDARD:is_open true ;
    B2AI_STANDARD:publication "https://doi.org/10.1016/j.jbi.2019.103175"^^xsd:anyURI ;
    B2AI_STANDARD:purpose_detail "FHIR is designed to enable healthcare information exchange across different systems and organizations, supporting clinical care delivery, research, and public health reporting." ;
    B2AI_STANDARD:requires_registration false ;
    B2AI_STANDARD:responsible_organization B2AI_ORG:50 ;
    B2AI_APP:category "B2AI_STANDARD:DataStandard" ;
    B2AI_APP:contribution_date "2024-12-20"^^xsd:date ;
    B2AI_APP:contributor_name "Harry Caufield" ;
    B2AI_APP:contributor_orcid "ORCID:0000-0001-5705-7831"^^xsd:anyURI ;
    B2AI_APP:has_application B2AI_APP:200,
        B2AI_APP:201 ;
    B2AI_APP:url "https://www.hl7.org/fhir/"^^xsd:anyURI .

B2AI_APP:200 schema1:description "AI-powered clinical decision support system that analyzes FHIR-formatted patient data to provide real-time recommendations for diagnosis and treatment planning." ;
    schema1:name "FHIR-based Clinical Decision Support" ;
    B2AI_APP:category "B2AI:Application" ;
    B2AI_APP:datasheet "https://example.org/cds-datasheet"^^xsd:anyURI ;
    B2AI_APP:references "https://doi.org/10.1093/jamia/example1"^^xsd:anyURI .

B2AI_APP:201 schema1:description "Machine learning application that processes FHIR-based electronic health records to predict patient readmission risk and adverse outcomes." ;
    schema1:name "FHIR Patient Risk Prediction" ;
    B2AI_APP:category "B2AI:Application" ;
    B2AI_APP:datasheet "https://example.org/risk-prediction-datasheet"^^xsd:anyURI ;
    B2AI_APP:references "https://doi.org/10.1038/s41746-example"^^xsd:anyURI .

